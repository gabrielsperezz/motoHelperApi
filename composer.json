{
    "name": "coderockr/silex-skel",
    "description": "MotoHelper",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "silex/silex": "v1.3.4",
        "twig/twig": "~1.20|~2.0",
        "symfony/twig-bridge": "v2.7.5",
        "monolog/monolog": "^1.17",
        "doctrine/dbal": "^2.5",
        "deralex/yaml-config-service-provider": "^1.0",
        "dflydev/doctrine-orm-service-provider": "1.0.*@dev",
        "doctrine/migrations": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "respect/validation": "^1.0",
        "symfony/console": "2.5",
        "symfony/translation": "~2.0"
    },
    "require-dev": {
        "symfony/browser-kit": "*",
        "phpunit/phpunit": "4.6.*",
        "squizlabs/php_codesniffer": "^2.0@dev",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload": {
        "psr-0": {
            "MotoHelper\\": "src/"
        }
    },
    "config": {
        "process-timeout": 9000,
        "secure-http":false
    }
}

    <script src="{{ app.lib_path }}/admin-lte/plugins/datatables/extensions/Responsive/js/dataTables.responsive.min.js" ></script>

    <script src="{{ app.asset_path }}/js/mapa_fulltrack_fullarm/telaMapa{{app.version}}.min.js"></script>

{% endblock %}



